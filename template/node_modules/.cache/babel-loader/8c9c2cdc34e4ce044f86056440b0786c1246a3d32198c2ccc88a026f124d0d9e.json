{"ast":null,"code":"export default {\n  name: 'manager_main',\n  data: () => ({\n    drawer: null,\n    user: \"\",\n    links: [{\n      text: '审核商品',\n      icon: 'mdi-inbox-arrow-down',\n      route: \"/manager/check\"\n    }, {\n      text: '登录日志',\n      icon: 'mdi-send',\n      route: \"/manager/log\"\n    }, {\n      text: '操作日志',\n      icon: 'mdi-delete',\n      route: \"/manager/actionlog\"\n    }]\n  }),\n  async created() {\n    await this.axios.get('userId2userName/', {\n      params: {\n        user_id: this.$store.state.userId\n      }\n    }).then(response => {\n      this.user = response.data.user_name;\n    }).catch(function (error) {\n      console.log(error);\n    });\n  }\n};","map":{"version":3,"mappings":"AAkEA;EACAA;EACAC;IACAC;IACAC;IACAC,QACA;MAAAC;MAAAC;MAAAC;IAAA,GACA;MAAAF;MAAAC;MAAAC;IAAA,GACA;MAAAF;MAAAC;MAAAC;IAAA;EAEA;EACA;IACA;MAAAC;QAAAC;MAAA;IAAA,GACAC;MACA;IACA,GACAC;MACAC;IACA;EACA;AACA","names":["name","data","drawer","user","links","text","icon","route","params","user_id","then","catch","console"],"sourceRoot":"src/view/manager","sources":["manager.vue"],"sourcesContent":["<!-- manager主页面 -->\r\n<template>\r\n  <div class=\"text-center\">\r\n    <v-app-bar\r\n        color=\"pink\"\r\n        width=\"10dp\"\r\n        elevation=\"3\"\r\n        dense\r\n        fixed\r\n    >\r\n      <v-app-bar-nav-icon\r\n          @click=\"drawer = true\">\r\n      </v-app-bar-nav-icon>\r\n    </v-app-bar>\r\n\r\n    <v-navigation-drawer\r\n        class=\"white\"\r\n        v-model=\"drawer\"\r\n        absolute temporary\r\n        fixed\r\n        app\r\n    >\r\n      <v-row\r\n          color=\"white\"\r\n          class=\"pa-4\"\r\n      >\r\n        <v-avatar color=\"pink\">\r\n            <span class=\"white--text text-h5\">{{ this.user }}</span>\r\n        </v-avatar>\r\n        <div>{{ this.user }}</div>\r\n      </v-row>\r\n\r\n      <v-divider></v-divider>\r\n\r\n      <v-list>\r\n        <v-list-item\r\n            v-for=\"link in links\"\r\n            :key=\"link.text\"\r\n            router :to=\"link.route\"\r\n        >\r\n          <v-list-item-action>\r\n            <v-icon color=\"pink\">{{link.icon}}</v-icon>\r\n          </v-list-item-action>\r\n          <v-list-item-content>\r\n            <v-list-item-title class=\"black--text\">{{link.text}}</v-list-item-title>\r\n          </v-list-item-content>\r\n        </v-list-item>\r\n      </v-list>\r\n\r\n      <template v-slot:append>\r\n        <div class=\"pa-2\">\r\n          <router-link to=\"/login/logout\">\r\n            <v-btn block>\r\n              退出登录\r\n            </v-btn>\r\n          </router-link>\r\n        </div>\r\n      </template>\r\n    </v-navigation-drawer>\r\n\r\n    <div style=\"height: 3rem;\"></div>\r\n    <router-view></router-view>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'manager_main',\r\n  data: () => ({\r\n    drawer: null,\r\n    user: \"\",\r\n    links: [\r\n      { text: '审核商品', icon: 'mdi-inbox-arrow-down', route: \"/manager/check\" },\r\n      { text: '登录日志', icon: 'mdi-send', route: \"/manager/log\" },\r\n      { text: '操作日志', icon: 'mdi-delete', route: \"/manager/actionlog\" },\r\n    ],\r\n  }),\r\n  async created() {\r\n    await this.axios.get('userId2userName/', { params: { user_id: this.$store.state.userId } })\r\n      .then((response) => {\r\n        this.user = response.data.user_name;\r\n      })\r\n      .catch(function (error) {\r\n        console.log(error);\r\n      });\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* 如果需要进一步的自定义样式，可以在这里添加 */\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}